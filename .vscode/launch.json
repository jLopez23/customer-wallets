{
  "configurations": [
    {
      "type": "node",
      "name": "Nestjs",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "args": [
        "${workspaceRoot}/src/main.ts"
      ],
      "autoAttachChildProcesses": true,
      "console": "integratedTerminal"
    }
  ]
}